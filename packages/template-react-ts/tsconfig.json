{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es5", "scripthost", "dom"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": false,
    "allowJs": true,
    "jsx": "react-jsx",
    "outDir": "./dist",
    "types": ["@creapp/config-react"]
  },
  "exclude": ["node_modules", "dist"]
}
